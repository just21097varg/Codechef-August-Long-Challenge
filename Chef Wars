#include <iostream>
using namespace std;

int main() {
	int t;
    cin>>t;
    while(t--)
    {
        long int n,m,s=0;
        cin>>n>>m;
        while(m!=0)
        {
            s=s+m;
            m=m/2;
        }
        if(s<n)
          cout<<"0"<<endl;
        else
          cout<<"1"<<endl;
    }
	return 0;
}
